teamb.s/        0           0     0     644     1068      `
#You are going to write two functions here:
# function_one
# function_two

.global function_one
function_one:
    PUSH {LR}
    PUSH {R4-R12}        @This preserves R4 through R12
	mov r4, r0	//in
	mov r5, r1	//out
	mov r6, r2	//width
	mov r7, r3	//height
	mov r8, #7	//counter
	mul r9, r6, r7 //total size
	mov r11, #0
	loop1:
	sub r8, #1
	ldrb r0, [r4,r8]
	strb r0, [r5,r11]
	add r11, #1
	cmp r8, #0
	beq quit
	b loop1


quit:
	MOV R0,#0			 @Return value of 0
    POP {R4-R12}         @Restore R4 through R12 for the calling function
	POP {PC}             @Return from a function
	
.global function_two
function_two:
    PUSH {LR}
    PUSH {R4-R12}        @This preserves R4 through R12 
  	mov r4, r0  //aray
    mov r5, r1  //size 1
    mov r6, r2  //stride 7
	mov r7, #0 //byte offset 4 for ints

    loop2:
	ldr r0, [r4,r7]
	lsl r0, #1
	str r0, [r4,r7]
	add r7, #4
	cmp r7, #28
	beq quit
	b loop2
	
quit2:
	//MOV R0, #0			 @Return value of 0
    POP {R4-R12}         @Restore R4 through R12 for the calling function
	POP {PC}             @Return from a function
